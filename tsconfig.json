{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
